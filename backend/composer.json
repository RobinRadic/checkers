{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "^4.0",
        "laradic/idea": "dev-master@dev",
        "laradic/service-provider": "dev-master@dev",
        "laradic/support": "dev-master@dev",
        "laravel/framework": "5.6.*",
        "laravel/passport": "~6.0",
        "laravel/tinker": "^1.0",
        "predis/predis": "1.1.*",
        "pusher/pusher-http-laravel": "^4.1",
        "pusher/pusher-php-server": "^3.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "repositories": [
        {"type": "path", "url": "../../../php/projects/laradic/workbench/laradic/*"}
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "cleandb": [
            "rm database/database.sqlite &> /dev/null",
            "touch database/database.sqlite"
        ],
        "reinstall": [
            "rm -rf vendor/ composer.lock",
            "@composer clear-cache",
            "@composer install",
            "@cleandb",
            "@php artisan migrate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
